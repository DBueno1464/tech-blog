<!-- Loop over each object in the projects array -->
{{#each posts as |post| }}
<div class="row mb-4 post">
    <div class="col-md-5">
        <h2>         
            <a href="/post/{{post.id}}">{{post.title}}</a>
        </h2>
    <div class="col-md-7">
        <p>
            {{post.content}}
        </p>
    </div>
        <p>Created by {{post.user.name}} on {{format_date post.date_created}}</p>
    </div>
</div>
{{/each}}

<script src="/js/logout.js"></script>